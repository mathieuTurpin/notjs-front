<% layout(layoutFile) %>

<div>
  <form id="venteForm">
    <div class="form-group">
      <input id="nomProd" type="text" placeholder="Nom du produit" class="form-control" required >
    </div>
    <div class="form-group">
      <input id="prixProd" type="text" placeholder="Prix" class="form-control" required >
    </div>
    <div class="form-group">
      <textarea id="descProd" placeholder="Description" class="form-control" required ></textarea>
    </div>
    <button type="submit" class="btn btn-success">Valider</button>
  </form>
  <h3>FriendName like this product:</h3>
  <ul id="result"></ul>
</div>